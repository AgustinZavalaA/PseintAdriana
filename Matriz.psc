Algoritmo Juana
	dimension matriz[4,4]
	para i = 0 hasta 3 Con Paso 1
		para j = 0 hasta 3 Con Paso 1
			imprimir "Dame un numero"
			leer matriz[i,j]
		FinPara
	FinPara
	para i = 0 hasta 3 Con Paso 1
		para j = 0 hasta 3 Con Paso 1
			imprimir "posicion [" i "," j "] = " matriz[i,j]
		FinPara
	FinPara
FinAlgoritmo
